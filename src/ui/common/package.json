    "@fortawesome/react-fontawesome": "^0.1.18",
    "@mui/lab": "*",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "autosuggest-highlight": "^3.2.1",
    "cron-parser": "^4.0.0",
    "query-string": "^7.1.1",
    "random-word-slugs": "^0.1.6",
    "react": "^17.0.2",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
